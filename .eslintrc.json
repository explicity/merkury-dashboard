{
	"extends": "airbnb",

	"rules": {
		"linebreak-style": 0,
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
		"no-shadow": 0,
		"no-undef": 0,
		"no-undef-init": 0,
		"no-undefined": 0,
		"no-tabs": 0,
		"comma-dangle": 0,
		"import/extensions": "off",
		"import/no-unresolved": "off",
		"react/require-default-props": 0,
		"no-plusplus": 0,
		"class-methods-use-this": 0
	}
}
